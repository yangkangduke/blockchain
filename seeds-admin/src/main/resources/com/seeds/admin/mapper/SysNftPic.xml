<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.seeds.admin.mapper.SysNftPicMapper">

    <select id="getListReceiptByMintAddress" resultType="java.lang.String">
        select list_receipt
        from z_market_order
        where status = 1
          and mint_address = #{tokenAddress}
    </select>
</mapper>
